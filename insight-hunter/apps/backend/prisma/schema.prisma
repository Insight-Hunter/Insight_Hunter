datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  demoMode  Boolean  @default(true)
  resetToken String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
